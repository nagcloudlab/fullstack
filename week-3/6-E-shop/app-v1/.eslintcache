m '../review-form-v2';\n\nimport * as api from '../../api'\n\nimport { useDispatch, useSelector } from 'react-redux'\n\nconst Item = ({ value: item }) => {\n\n    const [tab, setTab] = useState(1)\n\n    const cartLine = useSelector(state => state.cart[item.id])\n    const reviews = useSelector(state => state.reviews[item.id] || [])\n\n    const dispatch = useDispatch();\n\n\n    let qty = 0;\n    if (cartLine) {\n        qty = cartLine.qty;\n    }\n\n\n    const handleCartItemQty = (q, item) => {\n        if (qty === 0) return\n        let action = { type: 'CART_ITEM_QTY', item, qty: q } // sync action\n        dispatch(action)\n    }\n\n    useEffect(() => {\n        if (tab === 3) {\n            let action = api.getReviews(item.id) // async\n            dispatch(action)\n        }\n    }, [tab])\n\n    const handleBuy = () => {\n        let action = api.addToCart(item);\n        dispatch(action)// dispatch action to redux store\n    }\n\n    const handleNewReview = (review) => {\n        // let action = { type: 'ADD_NEW_REVIEW', review, itemId: item.id }\n        // dispatch(action) // sync action\n        let action = api.postNewReview(review, item.id)\n        dispatch(action) // async action\n    }\n\n    const renderBuyBtn = () => {\n        if (item.can_buy) {\n            return <button onClick={handleBuy} className=\"btn btn-lg btn-dark\">Add to cart</button>\n        }\n    }\n\n    const renderReviews = () => {\n        return reviews.map((rev, idx) => {\n            return <Review value={rev} key={idx} />\n        })\n    }\n\n    const renderTabPanel = () => {\n        switch (tab) {\n            case 1: {\n                return (<div>{item.description}</div>)\n            }\n            case 2: {\n                return (<div>Not Yet</div>)\n            }\n            case 3: {\n                return (\n                    <div>\n                        {renderReviews()}\n                        <div className=\"row\">\n                            <div className=\"col-md-8\">\n                                <ReviewForm onSubmit={handleNewReview} />\n                            </div>\n                        </div>\n                    </div>)\n            }\n            default: {\n                return null\n            }\n        }\n    }\n\n    return (\n        <div>\n            <div className=\"row\">\n                <div className=\"col-12 col-sm-3 col-md-3\">\n                    <img src={item.image_path} alt={item.name} className=\"img-fluid\" />\n                </div>\n                <div className=\"col-12 col-sm-6 col-md-6\">\n                    <h5>{item.name}</h5>\n                    <h6>&#8377;{item.price}</h6>\n                    <span className=\"mr-3\">{renderBuyBtn(item)}</span>\n\n                    <span className=\"m-3\">\n                        <span className=\"badge badge-warning\">\n                            <i onClick={e => handleCartItemQty(1, item)} className=\"fa fa-plus p-2\"></i>\n                        </span>\n                        <span className=\"m-2\">{qty}</span>\n                        <span className=\"badge badge-warning\">\n                            <i onClick={e => handleCartItemQty(-1, item)} className=\"fa fa-minus p-2\"></i>\n                        </span>\n                    </span>\n\n                    <div className=\"mt-5\"></div>\n                    <ul className=\"nav nav-tabs\">\n                        <li className=\"nav-item\">\n                            <a\n                                className={`nav-link ${tab === 1 ? 'active' : ''}`}\n                                href=\"/\"\n                                onClick={e => { e.preventDefault(); setTab(1) }}>\n                                Description\n                            </a>\n                        </li>\n                        <li className=\"nav-item\">\n                            <a\n                                className={`nav-link ${tab === 2 ? 'active' : ''}`}\n                                href=\"/\"\n                                onClick={e => { e.preventDefault(); setTab(2) }}>\n                                Specification\n                            </a>\n                        </li>\n                        <li className=\"nav-item\">\n                            <a\n                                className={`nav-link ${tab === 3 ? 'active' : ''}`}\n                                href=\"/\"\n                                onClick={e => { e.preventDefault(); setTab(3) }}>\n                                Reviews\n                            </a>\n                        </li>\n                    </ul>\n                    {renderTabPanel(item)}\n                </div>\n            </div>\n            <hr />\n        </div>\n    );\n};\n\nexport default Item;","/Users/nag/Desktop/fullstack/week-3/6-E-shop/app-v1/src/components/review/index.js",[],"/Users/nag/Desktop/fullstack/week-3/6-E-shop/app-v1/src/components/cart-view/index.js",[],"/Users/nag/Desktop/fullstack/week-3/6-E-shop/app-v1/src/components/home/index.js",[],"/Users/nag/Desktop/fullstack/week-3/6-E-shop/app-v1/src/components/not-found/index.js",[],"/Users/nag/Desktop/fullstack/week-3/6-E-shop/app-v1/src/components/login/index.js",[],"/Users/nag/Desktop/fullstack/week-3/6-E-shop/app-v1/src/components/review-form/index.js",[],"/Users/nag/Desktop/fullstack/week-3/6-E-shop/app-v1/src/components/review-form-v2/index.js",[],"/Users/nag/Desktop/fullstack/week-3/6-E-shop/app-v1/src/store/index.js",[],"/Users/nag/Desktop/fullstack/week-3/6-E-shop/app-v1/src/reducers/index.js",[],"/Users/nag/Desktop/fullstack/week-3/6-E-shop/app-v1/src/reducers/cart.js",[],"/Users/nag/Desktop/fullstack/week-3/6-E-shop/app-v1/src/components/cart-badge/index.js",[],"/Users/nag/Desktop/fullstack/week-3/6-E-shop/app-v1/src/reducers/reviews.js",[],"/Users/nag/Desktop/fullstack/week-3/6-E-shop/app-v1/src/api/index.js",[],"/Users/nag/Desktop/fullstack/week-3/6-E-shop